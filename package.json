{
  "name": "shopify-node-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prod": "yarn run clean && yarn run build && cross-env NODE_ENV=production yarn run start",
    "dev": "cross-env NODE_ENV=development yarn run start",
    "start": "NODE_ENV=production nodemon ./bin/www",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js --progress --profile --colors",
    "clean": "rm -rf ./assets && mkdir ./assets",
    "pretty": "prettier --single-quote --trailing-comma es5 --write {client,bin,config,server}{/*,/**/*}.js",
    "precommit": "yarn run pretty"
  },
  "engines": {
    "node": "8.11.4",
    "npm": "6.7"
  },
  "browsers": [
    "last 3 chrome versions",
    "last 3 firefox versions",
    "last 2 versions",
    "safari >= 8",
    "ios >= 8",
    "ie >= 11",
    "explorermobile >= 11",
    "android >= 4.4"
  ],
  "dependencies": {
    "@shopify/polaris": "^2.3.1",
    "@shopify/shopify-express": "^1.0.0-alpha.8",
    "acorn": "^6.0.7",
    "chalk": "^1.1.3",
    "connect-redis": "^3.3.0",
    "cross-env": "^5.2.0",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.9",
    "express": "^4.16.4",
    "express-session": "^1.15.3",
    "knex": "^0.13.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-object-inspector": "^0.2.1",
    "react-redux": "^5.1.1",
    "redis": "^2.7.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "shopify-api-node": "^2.18.1",
    "sqlite3": "4.0.2",
    "url": "^0.11.0",
    "webpack-middleware": "^1.5.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "3.19.0",
    "eslint-plugin-prettier": "^2.7.0",
    "global": "^4.3.2",
    "postcss-loader": "^2.1.6",
    "prettier": "^1.16.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.18.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-dev-server": "^2.11.3",
    "webpack-hot-middleware": "^2.24.3"
  }
}
